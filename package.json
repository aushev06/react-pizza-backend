    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/typeorm": "^8.0.2",
    "@ntegral/nestjs-sentry": "^3.0.3",
    "@sentry/node": "^6.11.0",
    "@t00nday/nestjs-pdf": "^2.0.6",
    "aws-cloudfront-sign": "^2.2.0",
    "aws-sdk": "^2.785.0",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.1",
    "bull": "^3.18.0",
    "cache-manager": "^3.4.0",
    "cache-manager-redis-store": "^2.0.0",
    "checkout-sdk-node": "^1.0.19",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "escape-html": "^1.0.3",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "mime-types": "^2.1.27",
    "multer-s3": "^2.9.0",
    "nest-winston": "^1.5.0",
    "nestjs-twilio": "^1.0.2",
    "nodemailer": "^6.4.14",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "sanitize-html": "^2.4.0",
    "slugify": "^1.4.6",
    "swagger-ui-express": "^4.1.6",
    "twilio": "^3.54.1",
    "typeorm": "0.2.32",
    "typeorm-naming-strategies": "^2.0.0",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.6",
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.14.4",
    "@types/cache-manager": "^3.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cron": "^1.7.2",
    "@types/ejs": "^3.0.3",
    "@types/express": "^4.17.3",
    "@types/faker": "^5.1.3",
    "@types/jest": "25.2.3",
    "@types/lodash": "^4.14.162",
    "@types/mime-types": "^2.1.0",
    "@types/multer-s3": "^2.7.8",
    "@types/node": "^13.9.1",
    "@types/passport": "^1.0.4",
    "@types/passport-facebook": "^2.1.10",
    "@types/passport-jwt": "^3.0.3",
    "@types/sanitize-html": "^2.3.1",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "husky": "^4.3.0",
    "jest": "26.0.1",
    "lint-staged": "^10.5.1",
    "node-mocks-http": "^1.9.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^1.19.1",
    "supertest": "^6.1.6",
    "ts-jest": "^26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5",
    "yarn": "^1.22.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "bracketSpacing": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
